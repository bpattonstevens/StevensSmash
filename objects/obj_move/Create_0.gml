// all these values are set by the actual move
percent = 0; // amount of damage move does
knockbackX = 0; // amount of pixels knocked away on X axis
knockbackY = 0; // amount of pixels knocked away on Y axis
hitstun = 0; // amount of frames character being hit is stunned for
moveLag = 0; // amount of frames character using move is "stunned" for

// the hitbox coords for the move
// relative to the center of the character
hitboxStartX = -20;
hitboxStartY = -5;
hitboxEndX = -10;
hitboxEndY = 5;

// frames in animation to start and end hitboxes
startFrame = 0;
endFrame = 0;